{
  "name": "localess",
  "version": "1.7.0",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.js",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "prebuild:prod": "ng g @lessify/angular-tools:set-env --prefix LOCALESS",
    "watch": "ng build --watch --configuration development",
    "emulator": "firebase emulators:start --import=./firebase-export --export-on-exit=./firebase-export",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "prettier": "prettier --check ./src/app",
    "prettier:fix": "prettier --write ./src/app",
    "build:stats": "ng build --configuration production --stats-json",
    "stats": "webpack-bundle-analyzer dist/localess/stats.json",
    "generate:github": "ng-openapi-gen -i api-docs/api.github.com.2022-11-28.yaml -o src/app/shared/generated"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.3",
    "@angular/cdk": "^16.2.2",
    "@angular/common": "^16.2.3",
    "@angular/compiler": "^16.2.3",
    "@angular/core": "^16.2.3",
    "@angular/fire": "^16.0.0",
    "@angular/forms": "^16.2.3",
    "@angular/material": "^16.2.2",
    "@angular/platform-browser": "^16.2.3",
    "@angular/platform-browser-dynamic": "^16.2.3",
    "@angular/router": "^16.2.3",
    "@ngrx/effects": "^16.3.0",
    "@ngrx/entity": "^16.3.0",
    "@ngrx/router-store": "^16.3.0",
    "@ngrx/store": "^16.3.0",
    "@ngrx/store-devtools": "^16.3.0",
    "bootstrap": "^5.3.2",
    "browser-detect": "^0.2.28",
    "file-saver-es": "^2.0.5",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular-eslint/builder": "~16.2.0",
    "@angular-eslint/eslint-plugin": "~16.2.0",
    "@angular-eslint/eslint-plugin-template": "~16.2.0",
    "@angular-eslint/schematics": "~16.2.0",
    "@angular-eslint/template-parser": "~16.2.0",
    "@angular/cli": "^16.2.1",
    "@angular/compiler-cli": "^16.2.3",
    "@lessify/angular-tools": "^14.0.2",
    "@types/file-saver-es": "^2.0.2",
    "@types/jasmine": "~3.10.7",
    "@types/node": "^18.17.14",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-rxjs": "^5.0.3",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-openapi-gen": "^0.50.4",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-eslint": "^16.1.2",
    "typescript": "~5.1.6",
    "webpack-bundle-analyzer": "^4.9.1"
  }
}
